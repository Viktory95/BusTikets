<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.TicketsEntity" table="tickets" schema="bus_tickets" catalog="postgres">
        <id name="ticketId">
            <column name="ticket_id" sql-type="int4" precision="-1" not-null="true"/>
            <generator class="identity"></generator>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" not-null="true"/>
        </property>
        <property name="surname">
            <column name="surname" sql-type="varchar" not-null="true"/>
        </property>
        <property name="surname2">
            <column name="surname2" sql-type="varchar" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="varchar" not-null="true"/>
        </property>
        <property name="birthdayNum">
            <column name="birthday_num" sql-type="varchar" not-null="true"/>
        </property>
        <property name="passNum">
            <column name="pass_num" sql-type="numeric" precision="-1" not-null="true"/>
        </property>
        <property name="passSeria">
            <column name="pass_seria" sql-type="numeric" precision="-1" not-null="true"/>
        </property>
        <property name="place">
            <column name="place" sql-type="varchar" not-null="true"/>
        </property>
        <property name="raceNum">
            <column name="race_num" sql-type="varchar" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="numeric" precision="-1" not-null="true"/>
        </property>
        <property name="raceId">
            <column name="race_id" sql-type="numeric" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>